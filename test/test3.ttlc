extern func void test_func(int x)

func int main(void)
    int i=0;
    // 普通に1から
	for i=0 to 10
    	test_func(i);
	next

    // 3ずつ飛ばして
	for i=12 to 0 step -3
    	test_func(i);
	next
	return 0;
endfunc

func void test_func(int x)
        if ((x%2)==0) then
    		messagebox(x, "偶数です");
        else
    		messagebox(x, "奇数です");
        endif
		return 0;
endfunc
