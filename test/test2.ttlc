extern func void test_func(int x)

func int main(void)
    int x=100;
	test_func(x);
    x=x+1;
	test_func(x);
    x=x+1;
	test_func(x);
    x=x+1;
	test_func(x);
    x=x+1;
	test_func(x);
    x=x+1;
	return 0;
endfunc

func void test_func(int x)
        if ((x%2)==0) then
    		messagebox(x, "偶数です");
        else
    		messagebox(x, "奇数です");
        endif
endfunc
